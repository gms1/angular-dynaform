    "@angular-dynaform/nativescript": {
      "root": "projects/angular-dynaform/nativescript",
      "sourceRoot": "projects/angular-dynaform/nativescript/src",
      "projectType": "library",
      "prefix": "adf-nativescript",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
