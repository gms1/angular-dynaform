{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es2016",
      "es2017",
      "dom"
    ],
    "outDir": "../tmp/aot",
    "target": "es2015",
    "module": "es2015",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "**/*.jit.ts"
  ],
  "angularCompilerOptions": {
    "skipMetadataEmit": true,
    "skipTemplateCodegen": false,
    "fullTemplateTypeCheck": false,
    "debug": false
  }
}
