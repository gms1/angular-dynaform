  "repository": {
    "type": "git",
    "url": "git+https://github.com/gms1/angular-dynaform.git"
  },
  "main": "bundles/adf.core.umd.js",
  "module": "index.es5.js",
  "es2015": "index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp build",
    "rebuild": "gulp rebuild",
    "watch": "gulp watch",
    "test": "gulp test",
    "testwatch": "gulp watch --task=test",
    "prepublishOnly": "echo \"ERROR: please use the dist-folder for publishing\" && exit 1",
    "release:build": "gulp clean && gulp build test --prod",
    "release:publish": "cd dist && npm --access public publish",
    "typedoc": "typedoc --options typedoc.json --out ../../docs/typedoc/core --tsconfig src/tsconfig.lib.json src/"
  },
