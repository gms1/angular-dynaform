#!/bin/bash

DN=$(dirname "$0")
BN=$(basename "$0")

PROJ_HOME=$(cd "${DN}/.."; pwd)
. "${DN}/sh/lib.sh"

#---------------------------------------------------------

CFGS=(
  projects/angular-dynaform/basic/src/lib/spec/test.config.ts
  projects/angular-dynaform/material/src/lib/spec/test.config.ts

  projects/basic-example/src/app/app.config.ts
  projects/material-example/src/app/app.config.ts
)


#---------------------------------------------------------
syncfileset "${CFGS[@]}" || exit 1
exit 0
